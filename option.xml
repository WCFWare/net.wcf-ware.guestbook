<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/option.xsd">
	<import>
		<categories>
			<category name="guestbook"></category>
			
			<category name="guestbook.general">
				<parent>guestbook</parent>
			</category>
		</categories>

		<options>
			<option name="module_guestbook">
				<categoryname>module.community</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>

			<option name="entry_max_length">
				<categoryname>guestbook.general</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>2000</defaultvalue>
				<minvalue>1</minvalue>
				<maxvalue>-1</maxvalue>
			</option>
			
			<option name="entries_per_page">
				<categoryname>guestbook.general</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue><![CDATA[10]]></defaultvalue>
			</option>
		</options>
	</import>
</data>